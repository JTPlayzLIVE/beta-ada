{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "start": "bundle exec jekyll serve",
    "start:detach": "bundle exec jekyll serve --config _config.yml,_config_test.yml --detach",
    "pa11y": "pa11y-ci --sitemap http://127.0.0.1:4000/sitemap.xml",
    "pa11y:json": "pa11y-ci --json --sitemap http://127.0.0.1:4000/sitemap.xml > ./pa11y-ci-results.json",
    "pa11y:save": "pa11y-ci-reporter-html --include-zero-issues -d ./pa11y-results",
    "test": "npm run pa11y",
    "test:ci": "npm-run-all --continue-on-error --serial start:detach pa11y:json pa11y:save"
  },
  "dependencies": {
    "netlify-cms": "^2.10.78",
    "uswds": "^2.11.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^2.4.0",
    "pa11y-ci-reporter-html": "^2.1.3"
  }
}
